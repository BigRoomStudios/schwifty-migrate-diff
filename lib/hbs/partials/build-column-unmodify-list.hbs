{{#each @root.columnDeltas.changes}}
{{#if (itemNotInArray this.model @root.createdTables)}}
{{#if (someObjectPropsHaveLength this)}}
    .alterTable('{{this.model}}', function(table) {
        {{#each (sort this.create)}}
        table.dropColumn('{{this}}');
        {{/each}}
        {{#each (sort (objectKeys this.alter))}}
        table.{{indexFromArray 0 (keyFromObject this ../this.alter)}}('{{this}}').alter();
        {{/each}}
        {{#each (sort this.drop)}}
        table.{{this.columnType}}('{{this.columnName}}');
        {{/each}}
    })
    {{/if}}
    {{/if}}
{{/each}}
