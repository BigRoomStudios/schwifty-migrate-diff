    // Handle column changes
{{#each columnDeltas}}
{{#each changes}}
    {{#if (itemNotInArray this.model @root.createdTables)}}
    knex.schema.alterTable('{{this.model}}', function(table) {

        {{#each this.create}}
        table.{{keyFromObject this (keyFromObject 'knexSchema' (keyFromObject ../this.model @root.allTables))}}('{{this}}');
        {{/each}}
        {{#each (objectKeys this.alter)}}
        table.{{keyFromObject this (keyFromObject 'knexSchema' (keyFromObject ../this.model @root.allTables))}}('{{this}}').alter();
        {{/each}}
        {{#each this.drop}}
        table.dropColumn('{{this}}')
        {{/each}}
    }),
    {{/if}}
{{/each}}
{{/each}}
